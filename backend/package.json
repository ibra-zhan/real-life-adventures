{
  "name": "sidequest-backend",
  "version": "1.0.0",
  "description": "SideQuest API Server - Transform daily moments into exciting challenges",
  "main": "dist/index.js",
  "scripts": {
    "build": "/opt/homebrew/bin/tsc",
    "start": "node -r tsconfig-paths/register dist/index.js",
    "dev": "concurrently \"/opt/homebrew/bin/tsc --watch\" \"nodemon -r tsconfig-paths/register dist/index.js\"",
    "dev:ts": "/opt/homebrew/bin/ts-node -r tsconfig-paths/register src/index.ts",
    "clean": "rm -rf dist",
    "type-check": "/opt/homebrew/bin/tsc --noEmit",
    "lint": "echo 'Linting not configured yet'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "sidequest",
    "gamification",
    "quests",
    "challenges",
    "api",
    "express",
    "typescript"
  ],
  "author": "SideQuest Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
